<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<AnchorPane id="root-pane" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindowFXController">
   <children>
      <ToolBar id="compare-tool-bar" nodeOrientation="LEFT_TO_RIGHT" styleClass="background" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <items>
            <Button id="compare-button" mnemonicParsing="false" onAction="#handleCompareButtonAction" />
            <Button id="copy-to-right-button" mnemonicParsing="false" onAction="#handleCopyToRightButtonAction" />
            <Button id="copy-to-left-button" mnemonicParsing="false" onAction="#handelCopyToLeftButtonAction" />
         </items>
      </ToolBar>
      <SplitPane fx:id="editorSplitPane" nodeOrientation="LEFT_TO_RIGHT" styleClass="background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
   </children>
    <stylesheets>
        <URL value="@css/MainWindow.css" />
    </stylesheets>
</AnchorPane>
